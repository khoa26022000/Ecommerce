<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_onboard"
    app:startDestination="@id/blankFragment">
    <fragment
        android:id="@+id/blankFragment"
        android:name="com.example.onboarding1.ui.BlankFragment"
        android:label="fragment_blank"
        tools:layout="@layout/fragment_blank" >
        <action
            android:id="@+id/action_blankFragment_to_splashTwoFragment"
            app:destination="@id/splashTwoFragment" />
    </fragment>
    <fragment
        android:id="@+id/splashTwoFragment"
        android:name="com.example.onboarding1.ui.SplashTwoFragment"
        android:label="fragment_splash_two"
        tools:layout="@layout/fragment_splash_two" >
        <action
            android:id="@+id/action_splashTwoFragment_to_splashThreeFragment"
            app:destination="@id/splashThreeFragment" />
    </fragment>
    <fragment
        android:id="@+id/splashThreeFragment"
        android:name="com.example.onboarding1.ui.SplashThreeFragment"
        android:label="fragment_splash_three"
        tools:layout="@layout/fragment_splash_three" />
</navigation>